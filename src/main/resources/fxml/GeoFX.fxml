<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="ipBox" alignment="TOP_CENTER">
         <children>
            <ImageView fx:id="imageView" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" />
            <TextField fx:id="ipText" />
            <Button fx:id="ipButton" mnemonicParsing="false" text="Check IP" />
         </children>
      </HBox>
      <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" side="BOTTOM" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab fx:id="locationTab" closable="false" text="Location">
            <content>
              
                
            </content>
          </Tab>
          <Tab fx:id="connectionTab" closable="false" text="Connection">
            <content>
              
                      
            </content>
          </Tab>
          <Tab fx:id="securityTab" closable="false" text="Security">
            <content>
              
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
